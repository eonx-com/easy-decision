{
    "name": "eonx-com/easy-decision",
    "description": "Your most complex decisions taken the easiest way",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "eonx-com/easy-pipeline": "^2.2",
        "symfony/expression-language": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "laravel/lumen-framework": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyDecision\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyDecision\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev"
        }
    }
}
