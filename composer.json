{
    "name": "loyaltycorp/easy-decision",
    "description": "Your most complex decisions taken the easiest way",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "stepthefkup/easy-pipeline": "^0.5",
        "symfony/expression-language": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "laravel/lumen-framework": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "LoyaltyCorp\\EasyDecision\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LoyaltyCorp\\EasyDecision\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.6-dev"
        }
    }
}
